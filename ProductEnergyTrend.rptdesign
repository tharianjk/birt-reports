}

function Productenergytrend()
{

	parent.selected_period_ndx=document.getElementById("s1").selectedIndex;
	parent.selected_sum_ndx=document.getElementById("s3").selectedIndex;
	parent.selected_fdate=document.getElementById("fromdate").value;
	parent.selected_tdate=document.getElementById("todate").value;

	if(parent.AssetTree.selectedtype == 5)
	{
		alert ("Select Section/Asset/Location!");
			return;
	}

	if(((document.getElementById("s1").selectedIndex==1) &amp;&amp; (document.getElementById("s3").selectedIndex==2))||
		((document.getElementById("s1").selectedIndex==1) &amp;&amp; (document.getElementById("s3").selectedIndex==3)))
	{
	
		alert ("Change the summarization method to Hour!");
			return;
	}
	if(document.getElementById("s1").selectedIndex==2 &amp;&amp; document.getElementById("s3").selectedIndex>1)
	{
		alert ("Change the summarization method to Hour!");
			return;
	
	}
	if(document.getElementById("s1").selectedIndex==3 &amp;&amp; document.getElementById("s3").selectedIndex>2)
	{
		alert ("Change the summarization method to Hour/Day!");
			return;
	
	}
	if ((document.getElementById("s1").selectedIndex==6) &amp;&amp; (document.getElementById("s3").selectedIndex==1)||
		(document.getElementById("s1").selectedIndex==6) &amp;&amp; (document.getElementById("s3").selectedIndex==2)||
		(document.getElementById("s1").selectedIndex==6) &amp;&amp; (document.getElementById("s3").selectedIndex==3)
		)
	{
		if ((document.getElementById("s1").selectedIndex==6) &amp;&amp; (document.getElementById("fromdate").value==''))
		{
			alert ("Enter a valid time period!");
			return;
		}
		else if ((document.getElementById("s1").selectedIndex==6)&amp;&amp; (document.getElementById("todate").value==''))
		{
			alert ("Enter a valid time period!");
			return;
		}
	
		else
		{
			var d1 = Date.parse(document.getElementById("todate").value);
			var d2 = Date.parse(document.getElementById("fromdate").value);
			if (d1 < d2) 
			{
				alert ("To Date cannot be Greater than From Date!");
				return;
			}
			else
			{
		
				if((document.getElementById("s1").selectedIndex==6) &amp;&amp; (document.getElementById("s3").selectedIndex==1))
				{	
					curT=document.getElementById("todate").value+" 00:00:00";
					LHour=document.getElementById("fromdate").value+" 00:00:00";
					var Cfrom=document.getElementById("fromdate").value;
					var Cto=document.getElementById("todate").value;
					newa=",' Product Energy Trend summarized by hour from "+Cfrom+" to " +Cto+"SH' as C4 from ems_product p"+
					" inner join ems_allocation_product a on p.product_id=a.PRODUCT_ID"+
					" inner join ems_section_summary s on a.assettree_id=s.ASSETTREE_ID"+
					" inner join fwk_assettree ast on ast.assettree_id=s.assettree_id"+ 
					" inner join ems_time_dim t on t.TIME_ID=s.TIME_ID"+
					" where t.dt_date>='"+LHour+"'"+
					" and t.dt_date<'"+curT+"'"+
					" and ast.company_id='"+parent.compid+"' ";	
					
				}
				else if((document.getElementById("s1").selectedIndex==6) &amp;&amp; (document.getElementById("s3").selectedIndex==2))
				{
					curT=document.getElementById("todate").value+" 00:00:00";
					LHour=document.getElementById("fromdate").value+" 00:00:00";
					var Cfrom=document.getElementById("fromdate").value;
					var Cto=document.getElementById("todate").value;
					newa=",' Product Energy Trend summarized by day from "+Cfrom+" to " +Cto+"SD' as C4 from ems_product p"+
					" inner join ems_allocation_product a on p.product_id=a.PRODUCT_ID"+
					" inner join ems_section_summary s on a.assettree_id=s.ASSETTREE_ID"+
					" inner join fwk_assettree ast on ast.assettree_id=s.assettree_id"+ 
					" inner join ems_time_dim t on t.TIME_ID=s.TIME_ID"+
					" where t.dt_date>='"+LHour+"'"+
					" and t.dt_date<'"+curT+"'"+
					" and ast.company_id='"+parent.compid+"' ";	
					
				}
				else if((document.getElementById("s1").selectedIndex==6) &amp;&amp; (document.getElementById("s3").selectedIndex==3))
				{
					curT=document.getElementById("todate").value+" 00:00:00";
					LHour=document.getElementById("fromdate").value+" 00:00:00";
					var Cfrom=document.getElementById("fromdate").value;
					var Cto=document.getElementById("todate").value;
					newa=",' Product Energy Trend summarized by month from "+Cfrom+" to " +Cto+"SM' as C4 from ems_product p"+
					" inner join ems_allocation_product a on p.product_id=a.PRODUCT_ID"+
					" inner join ems_section_summary s on a.assettree_id=s.ASSETTREE_ID"+
					" inner join fwk_assettree ast on ast.assettree_id=s.assettree_id"+ 
					" inner join ems_time_dim t on t.TIME_ID=s.TIME_ID"+
