	document.onload=populate();

function populate()
{	//Function which sets the previously selected parameters for the report.
	if(parent.selected_period_ndx==null || parent.selected_period_ndx==0)
	{
		parent.selected_period_ndx=0;
	}
	document.getElementById("s1").selectedIndex=parent.selected_period_ndx;	
	if(document.getElementById("s1").selectedIndex==4)
	{
		if (document.getElementById("s1").selectedIndex==4)
		{
	
			document.getElementById("fromdate").disabled=false;
			document.getElementById("todate").disabled=false;
				
			document.getElementById("fromdate").value=parent.selected_fdate;
			document.getElementById("todate").value=parent.selected_tdate;
				
				
		}
		else
		{
			document.getElementById("fromdate").value='';
			document.getElementById("todate").value='';
			document.getElementById("fromdate").disabled=true;
			document.getElementById("todate").disabled=true;
				
		}
			
	}
	
}	
	
	
	
function CheckDate()
{
	if (document.getElementById("s1").selectedIndex==4)
	{
	
				document.getElementById("fromdate").disabled=false;
				document.getElementById("todate").disabled=false;
				
				
	}
	else
	{
				document.getElementById("fromdate").value='';
				document.getElementById("todate").value='';
				document.getElementById("fromdate").disabled=true;
				document.getElementById("todate").disabled=true;
				
	}

}



function Consolidated()

{
	parent.selected_period_ndx=document.getElementById("s1").selectedIndex;
	parent.selected_fdate=document.getElementById("fromdate").value;
	parent.selected_tdate=document.getElementById("todate").value;
	if(parent.AssetTree.selectedtype == 5)
	{
		alert ("Select Section/Asset/Location!");
			return;
	}

	if ((document.getElementById("s1").selectedIndex==4) &amp;&amp; (document.getElementById("fromdate").value==''))
	{
		alert ("Enter a valid time period!");
		return;
	}
	else if ((document.getElementById("s1").selectedIndex==4)&amp;&amp; (document.getElementById("todate").value==''))
	{
		alert ("Enter a valid time period!");
		return;
	}
	
	
	else
	{
		var d1 = Date.parse(document.getElementById("todate").value);
		var d2 = Date.parse(document.getElementById("fromdate").value);
		if (d1 < d2) 
		{
			alert ("To Date cannot be Greater than From Date!");
			return;
		}
		else
		{
			curT=document.getElementById("todate").value+" 00:00:00";
			LHour=document.getElementById("fromdate").value+" 00:00:00";
			var Cfrom=document.getElementById("fromdate").value;
			var Cto=document.getElementById("todate").value;
			newa=",' Consolidated report from "+Cfrom+" to " +Cto+"' from vw_supplydemand SD inner join"+
				" ems_time_dim TD on td.day_id=sd.day_id where TD.dt_date>'"+LHour+"'"+
				" and td.dt_date <'"+curT+"'";
		
		}
			
	}
	
	


	if(document.getElementById("s1").selectedIndex==0)
	{
		alert("Select Duration!");
	}
	else
	{
		
	if(document.getElementById("s1").selectedIndex==1)
	{
		curT=year+ "-" + month + "-" + day+" "+hours+":"+minutes+":"+seconds;
		LHour=year7+ "-" + month7 + "-" + day7+" "+hours7+":"+minutes7+":"+seconds7;
		newa=",'7D' from vw_supplydemand SD inner join"+
				" ems_time_dim TD on td.day_id=sd.day_id where TD.dt_date>'"+LHour+"'"+
				" and td.dt_date <'"+curT+"'";
	}
	if(document.getElementById("s1").selectedIndex==2)
	{
	
		curT=year+ "-" + month + "-" + day+" "+hours+":"+minutes+":"+seconds;
		LHour=year30+ "-" + month30 + "-" + day30+" "+hours30+":"+minutes30+":"+seconds30;
		newa=",'30D' from vw_supplydemand SD inner join"+
				" ems_time_dim TD on td.day_id=sd.day_id where TD.dt_date>'"+LHour+"'"+
				" and td.dt_date <'"+curT+"'";
	}
	if(document.getElementById("s1").selectedIndex==3)
	{
		curT=year+ "-" + month + "-" + day+" "+hours+":"+minutes+":"+seconds;
		LHour=year365+ "-" + month365 + "-" + day365+" "+hours365+":"+minutes365+":"+seconds365;
