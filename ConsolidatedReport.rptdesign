				" ems_time_dim TD on td.day_id=sd.day_id where TD.dt_date>'"+LHour+"'"+
				" and td.dt_date <'"+curT+"'";
	}
	
	
	
	
	
	
	var cl1 = new String(location.href);
	var targetURL1 = new String();
	var tst1 = cl1.indexOf("&amp;newRPT");
