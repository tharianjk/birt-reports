            <method name="beforeOpen"><![CDATA[
if(params["newRPT"].value=="")
{
	this.queryText = this.queryText +
	
	",'7H' C6 from ems_section_summary s inner join fwk_assettree a on s.assettree_id=a.assettree_id"+
			" inner join fwk_section sec on s.assettree_id = sec.section_id"+
			" inner join ems_source src on sec.source_id = src.source_id"+
			" inner join EMS_TIME_DIM d ON S.TIME_ID=d.TIME_ID inner join ems_utility UT on UT.UTILITY_ID=s.UTILITY_ID"+
			" inner join ems_unit UN on UN.unit_id=UT.unit_id where"+ 
			" d.DT_DATE>='2015-03-30 00:00:00'and d.DT_DATE<'2015-03-31 00:00:00' and src.source_id='10001' group by src.sourcename,DAY_ID,d.DT_DATE, un.unitshort"+
			" UNION"+
			" select src.sourcename DISPLAYNAME, 0 as C2,DAY_ID,cast(d.dt_date as datetime) as C4,EUN.unitshort ,'7H' as C6"+
			" from ems_time_dim d , fwk_assettree a ,ems_meter m, ems_utility EU , ems_unit EUN , fwk_section sec, ems_source  src where m.parent_id=a.assettree_id and EU.utility_id=m.utility_id and EUN.unit_id=EU.unit_id and a.assettree_id = sec.section_id and sec.source_id = src.source_id and sec.source_id ='10001'"+ 
			" and d.DT_DATE>='2015-03-30 00:00:00' and d.DT_DATE<'2015-03-31 00:00:00' group by src.sourcename ,EUN.unitshort,DAY_ID,d.DT_DATE) tab group by DISPLAYNAME,DAY_ID,C4,unitshort,C6 order by C4 asc";

}
else
{


this.queryText = this.queryText+params["newRPT"]+" group by src.sourcename ,EUN.unitshort,DAY_ID,d.DT_DATE) tab group by DISPLAYNAME,DAY_ID,C4,unitshort,C6 order by C4 asc";
	
}]]></method>
