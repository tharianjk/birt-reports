  <Script>
function afterDataSetFilled( series, dataSet, icsc )
{
if( series.getSeriesIdentifier() == &quot;Series 1&quot; )
	{
		if( dataSet.getValues().length > 30 &amp;&amp; dataSet.getValues().length&lt;50)
		{
 			icsc.getChartInstance().getBlock().getBounds().setWidth(3800);
 			icsc.getChartInstance().getBlock().getBounds().setHeight(350);
		}
		else if(dataSet.getValues().length > 50)
		{
 			icsc.getChartInstance().getBlock().getBounds().setWidth(5800);
 			icsc.getChartInstance().getBlock().getBounds().setHeight(350);
		}
		else
		{
			icsc.getChartInstance().getBlock().getBounds().setWidth(800);
 			icsc.getChartInstance().getBlock().getBounds().setHeight(350);
		}
	}
}

